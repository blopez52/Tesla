<p id="notice"><%= notice %></p>

<h1>Listing Test Drives</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_drives.each do |test_drife| %>
      <tr>
        <td><%= link_to 'Show', test_drife %></td>
        <td><%= link_to 'Edit', edit_test_drife_path(test_drife) %></td>
        <td><%= link_to 'Destroy', test_drife, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test drive', new_test_drife_path %>
